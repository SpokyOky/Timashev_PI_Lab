@model Timashev_PI_Lab.Models.Product
@{
    ViewData["Title"] = "Удаление хим. элемента";
}

    <h1>Удаление хим. элемента</h1>

@using (Html.BeginForm("Удаление хим. элемента"))
{
    @Html.HiddenFor(model => model.Id)
    @Html.HiddenFor(model => model.Name)
    <p>Хим. Элемент: @Html.DropDownList("chemElementlist", (SelectList)ViewBag.ChemElementsList)</p>

    @Html.ValidationSummary()
    <p><input type="submit" value="Сохранить" /></p>
}
